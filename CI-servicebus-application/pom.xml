<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

    <modelVersion>4.0.0</modelVersion>

    <groupId>org.airtel.com</groupId>
    <artifactId>CI-servicebus-application</artifactId>
    <version>1.0-SNAPSHOT</version>

    <packaging>pom</packaging>

    <!-- The Oracle Service Bus Archetype creates an 'Application' containing two 
         'projects' - a 'normal' Oracle Service Bus project for building services,
         pipelines, etc., and a 'system' project for system resources.  If you do
         not need both projects, you may remove the one you do not need.  Or, if 
         you need multiple Oracle Service Bus projects, you can add more and update
         the module list below.
      -->
    <modules>
        <module>CI_Test1</module>
        <module>System</module>
        <module>CI_Test2</module>
         <!-- Assembly project -->
        <module>Assembly</module> 
    </modules>

</project>
